package _emulator;


public class palletHexPad {
    palletP j;
    public int t = 245;
    String type;
    boolean status;
    int mem = 0;
    int irq = 0; //0 = none, 1 = MI, 2 = NMI
    int kind = 0;

    


    public void setPal(palletP p) {
        j = p;
    }

    public void setMem(int m) {
        mem = m;
    }
}//hexpad
