    private void jButton1ActionPerformed(java.awt.event.ActionEvent evt) {//GEN-FIRST:event_jButton1ActionPerformed
        if(!processor.execute)
            processor.execute = true;
        else
            processor.execute = false;

        //fileParser f = new fileParser();
        //System.out.println(f.parseHex(jTextField1.getText(),jTextField1.getText().length()));
        //processor.PC = f.parseHex(jTextField1.getText(),jTextField1.getText().length());

        processor.updated = true;
    }//GEN-LAST:event_jButton1ActionPerformed

    private void jMenuItem1ActionPerformed(java.awt.event.ActionEvent evt) {//GEN-FIRST:event_jMenuItem1ActionPerformed
        try {
            int returnVal = jFileChooser1.showOpenDialog(this.getFrame());
            File file = jFileChooser1.getSelectedFile();

            fileParser parse = new fileParser();
            parse.parse(file, processor);

            jLabel14.setText(file.getName());
            processor.updated = true;
       }
       catch(Exception e) { }
       
    }//GEN-LAST:event_jMenuItem1ActionPerformed

    private void jButton2ActionPerformed(java.awt.event.ActionEvent evt) {//GEN-FIRST:event_jButton2ActionPerformed
        processor.step = true;
    }//GEN-LAST:event_jButton2ActionPerformed

    private void jButton3ActionPerformed(java.awt.event.ActionEvent evt) {//GEN-FIRST:event_jButton3ActionPerformed
        processor.PC = (processor.mem[0xFFFE] << 8);
        processor.PC = processor.PC | processor.mem[0xFFFF];
    }//GEN-LAST:event_jButton3ActionPerformed

    private void jButton4ActionPerformed(java.awt.event.ActionEvent evt) {//GEN-FIRST:event_jButton4ActionPerformed
        if(processor.stepThroughExecution)
            processor.stepThroughExecution = false;
        else
            processor.stepThroughExecution = true;

        processor.step = true;
        processor.updated = true;
    }//GEN-LAST:event_jButton4ActionPerformed

    private void jMenuItem5ActionPerformed(java.awt.event.ActionEvent evt) {//GEN-FIRST:event_jMenuItem5ActionPerformed
        JFrame mainFrame = _EmulatorApp.getApplication().getMainFrame();
        testInterface test = new testInterface();
        test.setLocationRelativeTo(mainFrame);
        test.setCPU(processor);

        _EmulatorApp.getApplication().show(test);
    }//GEN-LAST:event_jMenuItem5ActionPerformed

